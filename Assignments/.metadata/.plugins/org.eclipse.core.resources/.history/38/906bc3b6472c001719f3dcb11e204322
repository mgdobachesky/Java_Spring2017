
public class Lab4b {
	
	public static void main(String[] args) {
		// Define the lockers
		boolean[] lockers = new boolean[100];
	
		// Do this operation for each student
		for(int student = 0; student < 100; student++) {
			// Do this operation for appropriate lockers
			for(int locker = student; locker < 100; locker += student) {
				if(lockers[locker] == true) {
					lockers[locker] = false;
				} else if(lockers[locker] == false) {
					lockers[locker] = true;
				}
			}
		}
		
		for(int i = 0; i < 100; i++) {
			if(lockers[i] == true) {
				System.out.println("Locker " + (i + 1) + " is open");
			}
		}
	}
	
}
