package application;

public class Card {
	
	// Class Properties
	private int rank;
	private String suit;
	
	// Class Constructor
	public Card(int rank, String suit) {
		this.rank = rank;
		this.suit = suit;
	}
	
	// Getters
	public int getRank() {
		return this.rank;
	}
	
	public String getSuit() {
		return this.suit;
	}
	
	// Methods
	public void print() {
		System.out.println((getRank() + 1) + " of " + getSuit());
	}
}
