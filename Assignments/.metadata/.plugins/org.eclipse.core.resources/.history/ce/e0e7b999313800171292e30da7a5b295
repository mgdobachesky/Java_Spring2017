package application;

public class Card {
	
	// Define class properties
	private int value;
	private String suit;
	
	// Define class constructor
	public Card(int value, String suit) {
		this.value = value;
		this.suit = suit;
	}
	
	// Getters
	public int getValue() {
		return this.value;
	}
	
	public String getSuit() {
		return this.suit;
	}
	
	public void print() {
		System.out.println((getValue() + 1) + " of " + getSuit());
	}
}
