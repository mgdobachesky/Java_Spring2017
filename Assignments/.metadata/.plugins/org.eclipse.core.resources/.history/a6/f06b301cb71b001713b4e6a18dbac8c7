import java.util.*;

public class Lab2b {

	public static void main(String[] args) {
		// define scanner object to be used for input
		Scanner keyboard = new Scanner(System.in);
		int numberInput = 0;
		
		System.out.println("Enter an integer (0 ends the input): ");
		numberInput = keyboard.nextInt();
		
		System.out.printf("%d", numberInput);
		
		// close the Scanner object
		keyboard.close();
	}

}
